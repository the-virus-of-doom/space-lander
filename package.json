{
  "name": "space-lander",
  "version": "1.0.1",
  "description": "Lunar Lander inspired game made with Phaser.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-angular.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-angular/issues"
  },
  "homepage": "https://github.com/phaserjs/template-angular#readme",
  "keywords": [
    "phaser",
    "phaser3",
    "angular",
    "Angular"
  ],
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "prod": "ng serve --configuration production",
    "build": "ng build --configuration development --base-href='./'",
    "build:prod": "ng build --configuration production --base-href='./space-lander'",
    "build:vercel": "ng build --configuration production --base-href='./'",
    "build:clean": "rm -rf dist",
    "test": "ng test --browsers=ChromeHeadless",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless"
  },
  "dependencies": {
    "@angular/animations": "^19.2.18",
    "@angular/common": "^19.2.18",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.2.18",
    "@angular/forms": "^19.2.18",
    "@angular/platform-browser": "^19.2.18",
    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.2.18",
    "phaser": "^3.88.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.20",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^19.2.18",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.8"
  }
}
